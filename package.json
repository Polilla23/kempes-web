{
  "name": "kempes-web",
  "version": "1.0.0",
  "description": "Kempes Web created with NodeJs and React.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "cd backend && npm start",
    "migrate": "cd backend && npm run migrate",
    "generate": "cd backend && npm run generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Polilla23/kempes-web.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Polilla23/kempes-web/issues"
  },
  "homepage": "https://github.com/Polilla23/kempes-web#readme",
  "devDependencies": {
    "concurrently": "^9.2.0"
  },
  "dependencies": {
    "@fastify/multipart": "^9.0.3"
  }
}
